{
  "name": "task-app",
  "version": "2.1.1",
  "private": true,
  "description": "A simple task app built with React, Redux, TailwindCSS and SASS.",
  "homepage": "",
  "bugs": {
    "url": "https://github.com/lbajda/react-task-app/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lbajda/react-task-app.git"
  },
  "license": "MIT",
  "author": "Luke Bajda",
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "start": "react-scripts start",
    "test": "react-scripts test"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "wesbos"
    ],
    "rules": {
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": true
        }
      ],
      "react/prop-types": 0
    }
  },
  "dependencies": {
    "@fontsource/poppins": "^4.5.0",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@reduxjs/toolkit": "^1.7.1",
    "date-fns": "^2.28.0",
    "framer-motion": "^5.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-hot-toast": "^2.2.0",
    "react-icons": "^4.3.1",
    "react-redux": "^7.2.6",
    "react-scripts": "5.0.0",
    "sass": "^1.68.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "eslint-config-wesbos": "^3.0.2",
    "tailwindcss": "^3.3.3"
  }
}
